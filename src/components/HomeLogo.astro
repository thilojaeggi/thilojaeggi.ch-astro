<style>
    svg {
        pointer-events: none;
    }

    .logo-part {
        pointer-events: auto;
        opacity: 0;
        animation: slideUp 1s forwards;
        transition:
            fill 0.3s ease-in-out,
            stroke 0.3s ease-in-out;
        fill: currentColor;
        stroke: transparent;
        stroke-width: 0.3rem; /* Adjust thickness as needed matching the scale */
    }

    /* Trigger hover effect on the SVG container */
    svg:hover .logo-part {
        fill: transparent;
        stroke: currentColor;
    }

    .logo-part:nth-child(1) {
        animation-delay: 0.2s;
    }

    .logo-part:nth-child(2) {
        animation-delay: 0.3s;
    }

    .logo-part:nth-child(3) {
        animation-delay: 0.4s;
    }

    @keyframes slideUp {
        0% {
            transform: translateY(50px);
            opacity: 0;
        }
        100% {
            transform: translateY(0);
            opacity: 1;
        }
    }
</style>
